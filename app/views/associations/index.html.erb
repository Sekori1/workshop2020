<p id="notice"><%= notice %></p>

<h1>Associations</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Zipcode</th>
      <th>City</th>
      <th>Lat</th>
      <th>Long</th>
      <th>Phone</th>
      <th>Email</th>
      <th>Members</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @associations.each do |association| %>
      <tr>
        <td><%= association.name %></td>
        <td><%= association.address %></td>
        <td><%= association.zipcode %></td>
        <td><%= association.city %></td>
        <td><%= association.lat %></td>
        <td><%= association.long %></td>
        <td><%= association.phone %></td>
        <td><%= association.email %></td>
        <td><%= association.members %></td>
        <td><%= link_to 'Show', association %></td>
        <td><%= link_to 'Edit', edit_association_path(association) %></td>
        <td><%= link_to 'Destroy', association, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Association', new_association_path %>
